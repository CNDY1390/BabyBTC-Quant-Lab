{
  "rewrites": [
    {
      "source": "/app/(.*)",
      "destination": "/frontend/$1"
    },
    {
      "source": "/app",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/landingpage/$1"
    }
  ],
  "buildCommand": "cd landingpage && pnpm install && pnpm build && cd ../frontend && pnpm install && pnpm build",
  "outputDirectory": ".",
  "framework": null,
  "functions": {
    "landingpage/dist/**": {
      "excludeFiles": "**"
    },
    "frontend/dist/**": {
      "excludeFiles": "**"
    }
  }
}
