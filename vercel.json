{
  "buildCommand": "pnpm install && pnpm build",
  "outputDirectory": "public",
  "framework": null,
  "rewrites": [
    {
      "source": "/app",
      "destination": "/app/index.html"
    },
    {
      "source": "/app/(.*)",
      "destination": "/app/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
